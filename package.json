{
  "name": "lock-in",
  "version": "1.0.41",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:testflight": "eas build --platform ios --profile testflight --clear-cache",
    "verify:healthkit": "node scripts/verify-healthkit-config.js",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "clean": "if exist node_modules\\.cache rmdir /s /q node_modules\\.cache && if exist .expo rmdir /s /q .expo && npx expo install --fix",
    "test": "node tests/runner.js unit",
    "test:integration": "node tests/runner.js integration",
    "test:weekly": "node tests/runner.js weekly",
    "test:full": "npm run test && npm run test:integration"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@kingstinct/react-native-healthkit": "^13.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.3.1",
    "@supabase/supabase-js": "^2.45.0",
    "@tanstack/react-query": "^5.51.1",
    "expo": "~54.0.28",
    "expo-auth-session": "~7.0.10",
    "expo-background-fetch": "~14.0.9",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.11",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-notifications": "~0.32.14",
    "expo-router": "~6.0.18",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-task-manager": "~14.0.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "^16.0.0",
    "react-native-nitro-modules": "^0.32.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-worklets": "^0.5.1",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "expo": {
    "install": {
      "exclude": [
        "react"
      ]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-health"
        ]
      }
    }
  },
  "overrides": {
    "@expo/fingerprint": "0.15.4"
  },
  "private": true
}
