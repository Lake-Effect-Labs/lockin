{
  "expo": {
    "name": "Lock-In",
    "slug": "lock-in",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "lockin",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.lockin.app",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Lock-In needs access to your health data to track your fitness metrics and compete in leagues.",
        "NSHealthUpdateUsageDescription": "Lock-In needs permission to update your health data.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.background-delivery": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#FF6B35"
        }
      ],
      [
        "react-native-health",
        {
          "isClinicalDataEnabled": false
        }
      ],
      "expo-background-fetch",
      "expo-task-manager"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8616c2a3-a835-4695-bb82-26a29b6081eb"
      }
    }
  }
}
